{
	"module" : "Geo Ingestion Module",
	"input_params" : ["database", "schema", "archive"],
	"vars"   : {
		"script" : "$ROOT/../data_acquisition/geo/ingest-data.sh"
	},
	"actions" : [
		{ 
			"action"      : "perform-ingestion",
			"description" : "perform SHP files ingestion from given archive.",
			"command"     : "$script -d '$database' -s '$schema' -a '$archive'"
		}
	],
	"triggers" : [],
	"begin" : "print 'Begin Geo Ingestion'; return {'schema' : [_schema] }",
	"end"   : "print 'End Geo Ingestion'",
	"error" : "print 'Error while performing Geo ingestion'"
}